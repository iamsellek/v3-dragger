{
  "name": "v3-dragger",
  "version": "2.7.5",
  "description": "A vue toolset that let's you make any element draggable, on both mobile and desktop with just 3 lines of code!",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage && google-chrome coverage/lcov-report/index.html",
    "test:watch": "jest --watch",
    "build": "tsc -p .",
    "build:watch": "tsc -p . --watch",
    "prepublishOnly": "npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iamsellek/v3-dragger"
  },
  "homepage": "https://github.com/iamsellek/v3-dragger#readme",
  "keywords": [
    "vue",
    "drag",
    "mobile",
    "desktop",
    "touch"
  ],
  "author": "Chris Sellek",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "vue": "^3.3.4"
  }
}
